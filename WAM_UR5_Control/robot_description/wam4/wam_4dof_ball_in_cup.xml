<!-- ======================================================

    The Barrett WAM 4 DoF in 'ball in cup' setup as in the IAS Lab @ TU Darmstadt

    === HOW TO USE ===
    Drag and drop into MuJoCo, or use any MuJoCo API. In case your model gets unstable, 
    try to lower your timestep.

    === HOW TO MODIFY ===
    Most properties can be changed in the 'default' section. It is not possible to create 
    defaults for bodys and inertials, so you will have to search and replace for:
        - link length: third value of pos in bodys
        - link mass and inertia: all values of the inertials

    Contact:    mail@kaiploeger.net
	Last edit:  14 Apr 20

====================================================== -->


<mujoco model="wam_4dof_ball_in_cup">

    <option integrator="Euler" timestep="0.0002" viscosity="0.005"/>
    <compiler coordinate="local" angle="radian" meshdir="meshes/"/>
    
    
    <default>
        <joint type="hinge" limited="true" pos="0 0 0" axis="0 0 1" frictionloss="0.1"/>
        <site size=".02" rgba="1 0 0 0"/>
        <default class="viz">
            <geom type="mesh" contype="0" conaffinity="0" group="2" rgba="0.7 0.7 0.7 1" density="0"/>
        </default>
        <default class="col">
            <geom type="mesh" contype="0" conaffinity="1" group="3" rgba="0.5 0.6 0.7 1" density="0"/>
        </default>
        <default class="x_axis">
            <geom contype="0" conaffinity="0" group="1" rgba="1 0 0 1"
                  type="cylinder" size=".01 0.15" pos="0.15 0 0" euler="0 1.57 0"/>
        </default>
        <default class="y_axis">
            <geom contype="0" conaffinity="0" group="1" rgba="0 1 0 1"
                  type="cylinder" size=".01 0.15" pos="0 0.15 0" euler="1.57 0 0"/>
        </default>
        <default class="z_axis">
            <geom contype="0" conaffinity="0" group="1" rgba="0 0 1 1"
                  type="cylinder" size=".01 0.15" pos="0 0 0.15" euler="0 0 0"/>
          </default>

        <default class="joint_y">
            <joint pos="-0.00535 0 0" axis="0 1 0" group="3" limited="false" damping="0.0001" frictionloss="0.1"/>
        </default>

        <default class="joint_z">
            <joint pos="-0.00535 0 0" axis="0 0 1" group="3" limited="false" damping="0.0001" frictionloss="0.1"/>
        </default>
        
        <default class="chain_piece">         
            <geom size="0.001 0.00427" quat="0.707107 0 0.707107 0" type="capsule" contype="0" conaffinity="1" 
                group="3" rgba="0.5 0.6 0.7 1" density="0"/>
        </default>
        
        <default class="ball">
            <geom type="sphere" size="0.02" mass="0.021" contype="0" conaffinity="1" group="3" rgba="0.5 0.6 0.7 1"/>
        </default>

        <default class="chain_piece_visual">
            <geom size="0.001 0.00535" quat="0.707107 0 0.707107 0" type="capsule" contype="0" conaffinity="0" 
                group="2" rgba="0.7 0.7 0.7 1" density="0"/>
        </default>

        <default class="ball_visual">
            <geom type="sphere" size="0.02" contype="0" conaffinity="0" group="2" rgba="0.7 0.7 0.7 1" density="0"/>
        </default>
    </default>


